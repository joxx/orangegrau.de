{
	"name": "orangegrau,de - blog",
	"version": "9.0.0",
	"description": "A starter repository for the orangegrau.de blog web site using the Eleventy site generator.",
	"type": "module",
	"scripts": {
		"build": "npx @11ty/eleventy",
		"build-nocolor": "cross-env NODE_DISABLE_COLORS=1 npx @11ty/eleventy",
		"start": "npx @11ty/eleventy --serve --quiet",
		"debug": "cross-env DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "cross-env DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/joxx/orangegrau.de.git"
	},
	"author": {
		"name": "Joachim Kliemann",
		"email": "joachimkliemann@gmail.com",
		"url": "https://orangegrau.de/"
	},
	"license": "MIT",
	"engines": {
		"node": ">=18"
	},
	"homepage": "https://github.com/joxx/orangegrau.de#readme",
	"devDependencies": {
		"@11ty/eleventy": "^3.1.2",
		"@11ty/eleventy-img": "^6.0.4",
		"@11ty/eleventy-navigation": "^1.0.4",
		"@11ty/eleventy-plugin-rss": "^2.0.4",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.1",
		"cross-env": "^7.0.3",
		"luxon": "^3.6.1",
		"prismjs": "^1.30.0"
	},
	"dependencies": {
		"@zachleat/heading-anchors": "^1.0.3"
	}
}
